{
	"name": "frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"add-projects": "node scripts/add-projects.js",
		"check-duplicates": "node scripts/check-duplicates.js",
		"remove-duplicates": "node scripts/remove-duplicates.js",
		"add-images": "node scripts/add-missing-images.js",
		"studio": "cd ../studio && npm run dev"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-vercel": "^5.10.3",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"autoprefixer": "^10.4.21",
		"postcss": "^8.5.6",
		"sass": "^1.93.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"@portabletext/svelte": "^3.0.1",
		"@sanity/client": "^7.11.2",
		"@sveltejs/adapter-static": "^3.0.10",
		"gsap": "^3.13.0",
		"node-fetch": "^3.3.2",
		"pixi.js": "^8.13.2"
	}
}
